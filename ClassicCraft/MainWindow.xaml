                                </TabItem>
                            </TabControl>
                        </StackPanel>
                    </ScrollViewer>
                </TabItem>
                <TabItem Header="Player setup">
                    <ScrollViewer VerticalScrollBarVisibility="Visible" Padding="4">
                        <StackPanel>
                            <Grid HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="150" />
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ComboBox Text="player.json" Grid.Column="1" SelectedIndex="0" Margin="0 0 10 0" IsEditable="True" IsTextSearchEnabled="True" VerticalAlignment="Center" IsEnabled="False" />
                                <Button Content="save" Click="SavePlayerClick" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                <Button Content="load" Click="LoadPlayerClick" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                <Button Content="new" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False" />
                                <Button Content="copy" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="5" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False" />
                            </Grid>
                            <TabControl x:Name="PlayerTabs" Margin="0 5 0 0" SelectedIndex="0" Grid.Row="0" Grid.ColumnSpan="2">
                                <TabItem Header="Player">
                                    <StackPanel Margin="2">
                                        <Grid Margin="0 0 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="100" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Level" Grid.Column="0" />
                                            <ComboBox x:Name="Level" Grid.Column="1" SelectedIndex="1">
                                                <ComboBoxItem>60</ComboBoxItem>
                                                <ComboBoxItem>70</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="100" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Race" Grid.Column="0" />
                                            <ComboBox x:Name="Race" Grid.Column="1" SelectedIndex="4">
                                                <ComboBoxItem>Blood Elf</ComboBoxItem>
                                                <ComboBoxItem>Draenei</ComboBoxItem>
                                                <ComboBoxItem>Dwarf</ComboBoxItem>
                                                <ComboBoxItem>Gnome</ComboBoxItem>
                                                <ComboBoxItem>Human</ComboBoxItem>
