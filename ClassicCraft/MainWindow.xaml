<Window x:Class="ClassicCraftGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClassicCraftGUI"
        mc:Ignorable="d"
        Title="ClassicCraft" Height="600" Width="900" MinHeight="0" MinWidth="0">
    <Border Padding="10" Background="#222222" CornerRadius="2" Margin="0 0 0 0">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <TabControl x:Name="Tabs" SelectedIndex="1" Grid.Row="0" Grid.ColumnSpan="2">
                <TabItem Header="News" IsEnabled="False">
                    <ScrollViewer VerticalScrollBarVisibility="Visible" Padding="4">
                    </ScrollViewer>
                </TabItem>
                <TabItem Header="Simulation setup">
                    <ScrollViewer VerticalScrollBarVisibility="Visible" Padding="4">
                        <StackPanel>
                            <Grid HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="150" />
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <ComboBox Text="sim.json" Grid.Column="1" SelectedIndex="0" Margin="0 0 10 0" IsEditable="True" IsTextSearchEnabled="True" VerticalAlignment="Center" IsEnabled="False" />
                                <Button Content="save" Click="SaveSimConfigClick" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                <Button Content="load" Click="LoadSimConfigClick" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="3" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                <Button Content="new" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="4" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False" />
                                <Button Content="copy" Margin="0,2,10,2" Padding="3 0 3 0" Grid.Column="5" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False"/>
                            </Grid>

                            <TabControl x:Name="ConfigTabs"  Margin="0 5 0 0" SelectedIndex="0" Grid.Row="0" Grid.ColumnSpan="2">
                                <TabItem Header="Simulation configuration">
                                    <StackPanel Margin="2">
                                        <Grid  Margin="0 0 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Full Fight Log (10 sim max)" ToolTip="If enabled, the simulation will log every action (disables target error and stats weights options)" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox x:Name="LogFight" SelectionChanged="LogFight_SelectionChanged" Grid.Column="1" SelectedIndex="0">
                                                <ComboBoxItem>Disabled</ComboBoxItem>
                                                <ComboBoxItem>Enabled</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Target error %" ToolTip="" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox x:Name="TargetError" Width="100" IsEditable="True" IsTextSearchEnabled="True" PreviewTextInput="NumberCheck" SelectionChanged="TargetError_SelectionChanged" Grid.Column="1" SelectedIndex="1" HorizontalAlignment="Left">
                                                <ComboBoxItem>Fixed number</ComboBoxItem>
                                                <ComboBoxItem>0.5</ComboBoxItem>
                                                <ComboBoxItem>0.4</ComboBoxItem>
                                                <ComboBoxItem>0.3</ComboBoxItem>
                                                <ComboBoxItem>0.2</ComboBoxItem>
                                                <ComboBoxItem>0.1</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Nb Simulations" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox x:Name="NbSim" IsEditable="True" IsTextSearchEnabled="True" IsEnabled="false" Grid.Column="1" SelectedIndex="4">
                                                <ComboBoxItem>1</ComboBoxItem>
                                                <ComboBoxItem>10</ComboBoxItem>
                                                <ComboBoxItem>100</ComboBoxItem>
                                                <ComboBoxItem>1000</ComboBoxItem>
                                                <ComboBoxItem>10000</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Stats Weights" ToolTip="If enabled, simulation will do iterations to calculate each stat weight (disables detailed fight stats)" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox x:Name="StatsWeights" SelectedIndex="0" Grid.Column="1">
                                                <ComboBoxItem>Enable</ComboBoxItem>
                                                <ComboBoxItem>Disable</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                    </StackPanel>
                                </TabItem>
                                <TabItem Header="Fight configuration">
                                    <StackPanel Margin="2">
                                        <Grid Margin="0 0 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Fight length (seconds)" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox IsEditable="True" x:Name="FightLength" PreviewTextInput="NumberCheck" Grid.Column="1" SelectedIndex="3">
                                                <ComboBoxItem>60</ComboBoxItem>
                                                <ComboBoxItem>120</ComboBoxItem>
                                                <ComboBoxItem>180</ComboBoxItem>
                                                <ComboBoxItem>240</ComboBoxItem>
                                                <ComboBoxItem>300</ComboBoxItem>
                                                <ComboBoxItem>600</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Fight length variation %" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox IsEditable="True" x:Name="FightLengthVariation" PreviewTextInput="NumberCheck" Grid.Column="1" SelectedIndex="1">
                                                <ComboBoxItem>0</ComboBoxItem>
                                                <ComboBoxItem>10</ComboBoxItem>
                                                <ComboBoxItem>20</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <Grid Margin="0 10 0 0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="200" />
                                                <ColumnDefinition Width="100" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="Number of targets (BETA)" TextWrapping="Wrap" Grid.Column="0" />
                                            <ComboBox IsEditable="True" x:Name="NbTargets" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="0">
                                                <ComboBoxItem>1</ComboBoxItem>
                                                <ComboBoxItem>2</ComboBoxItem>
                                                <ComboBoxItem>3</ComboBoxItem>
                                                <ComboBoxItem>4</ComboBoxItem>
                                                <ComboBoxItem>10</ComboBoxItem>
                                            </ComboBox>
                                        </Grid>
                                        <TextBlock Text="Boss" Margin="0 5 0 0" />
                                        <Expander IsExpanded="True">
                                            <Border Margin="2" Padding="2" BorderBrush="#D6D6D6" BorderThickness="1" Background="#F2F2F2" CornerRadius="2">
                                                <StackPanel>
                                                    <Grid Margin="0 0 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Boss Level" TextWrapping="Wrap" Grid.Column="0" />
                                                        <ComboBox IsEditable="True" x:Name="BossLevel" PreviewTextInput="WholeNumberCheck" SelectedIndex="1" Grid.Column="1">
                                                            <ComboBoxItem>60</ComboBoxItem>
                                                            <ComboBoxItem>63</ComboBoxItem>
                                                        </ComboBox>
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Boss Life Mode" TextWrapping="Wrap" Grid.Column="0" />
                                                        <ComboBox x:Name="BossLifeMode" SelectionChanged="BossLifeMode_SelectionChanged" Grid.Column="1" SelectedIndex="0" Height="22" VerticalAlignment="Top">
                                                            <ComboBoxItem>Auto</ComboBoxItem>
                                                            <ComboBoxItem>Low life timer</ComboBoxItem>
                                                        </ComboBox>
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Boss 20% life at (seconds)" TextWrapping="Wrap" Grid.Column="0" />
                                                        <TextBox x:Name="BossLowLifeTime" PreviewTextInput="NumberCheck" IsEnabled="False" Grid.Column="1" />
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Boss Armor" TextWrapping="Wrap" Grid.Column="0" />
                                                        <ComboBox IsEditable="True" x:Name="BossArmor" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                            <ComboBoxItem>0</ComboBoxItem>
                                                            <ComboBoxItem>4400</ComboBoxItem>
                                                            <ComboBoxItem>6193</ComboBoxItem>
                                                            <ComboBoxItem>6400</ComboBoxItem>
                                                            <ComboBoxItem>7684</ComboBoxItem>
                                                        </ComboBox>
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Boss Magic Resistances" TextWrapping="Wrap" Grid.Column="0" />
                                                        <ComboBox IsEditable="True" x:Name="BossAllResist" TextBoxBase.TextChanged="AllResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                            <ComboBoxItem>Custom</ComboBoxItem>
                                                            <ComboBoxItem>0</ComboBoxItem>
                                                            <ComboBoxItem>50</ComboBoxItem>
                                                            <ComboBoxItem>100</ComboBoxItem>
                                                            <ComboBoxItem>200</ComboBoxItem>
                                                        </ComboBox>
                                                    </Grid>
                                                    <Expander IsExpanded="False" Margin="0 -5 0 0">
                                                        <Border Margin="2" Padding="2" BorderBrush="#D6D6D6" BorderThickness="1" Background="#F2F2F2" CornerRadius="2">
                                                            <StackPanel>
                                                                <Grid Margin="0 0 0 0">
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="200" />
                                                                        <ColumnDefinition Width="100" />
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Text="Arcane" TextWrapping="Wrap" Grid.Column="0" />
                                                                    <ComboBox IsEditable="True" x:Name="BossArcaneResist" TextBoxBase.TextChanged="BossResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                                        <ComboBoxItem>0</ComboBoxItem>
                                                                        <ComboBoxItem>50</ComboBoxItem>
                                                                        <ComboBoxItem>100</ComboBoxItem>
                                                                        <ComboBoxItem>200</ComboBoxItem>
                                                                    </ComboBox>
                                                                </Grid>
                                                                <Grid Margin="0 5 0 0">
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="200" />
                                                                        <ColumnDefinition Width="100" />
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Text="Fire" TextWrapping="Wrap" Grid.Column="0" />
                                                                    <ComboBox IsEditable="True" x:Name="BossFireResist" TextBoxBase.TextChanged="BossResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                                        <ComboBoxItem>0</ComboBoxItem>
                                                                        <ComboBoxItem>50</ComboBoxItem>
                                                                        <ComboBoxItem>100</ComboBoxItem>
                                                                        <ComboBoxItem>200</ComboBoxItem>
                                                                    </ComboBox>
                                                                </Grid>
                                                                <Grid Margin="0 5 0 0">
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="200" />
                                                                        <ColumnDefinition Width="100" />
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Text="Frost" TextWrapping="Wrap" Grid.Column="0" />
                                                                    <ComboBox IsEditable="True" x:Name="BossFrostResist" TextBoxBase.TextChanged="BossResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                                        <ComboBoxItem>0</ComboBoxItem>
                                                                        <ComboBoxItem>50</ComboBoxItem>
                                                                        <ComboBoxItem>100</ComboBoxItem>
                                                                        <ComboBoxItem>200</ComboBoxItem>
                                                                    </ComboBox>
                                                                </Grid>
                                                                <Grid Margin="0 5 0 0">
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="200" />
                                                                        <ColumnDefinition Width="100" />
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Text="Light" TextWrapping="Wrap" Grid.Column="0" />
                                                                    <ComboBox IsEditable="True" x:Name="BossLightResist" TextBoxBase.TextChanged="BossResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                                        <ComboBoxItem>0</ComboBoxItem>
                                                                        <ComboBoxItem>50</ComboBoxItem>
                                                                        <ComboBoxItem>100</ComboBoxItem>
                                                                        <ComboBoxItem>200</ComboBoxItem>
                                                                    </ComboBox>
                                                                </Grid>
                                                                <Grid Margin="0 5 0 0">
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="200" />
                                                                        <ColumnDefinition Width="100" />
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Text="Nature" TextWrapping="Wrap" Grid.Column="0" />
                                                                    <ComboBox IsEditable="True" x:Name="BossNatureResist" TextBoxBase.TextChanged="BossResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                                        <ComboBoxItem>0</ComboBoxItem>
                                                                        <ComboBoxItem>50</ComboBoxItem>
                                                                        <ComboBoxItem>100</ComboBoxItem>
                                                                        <ComboBoxItem>200</ComboBoxItem>
                                                                    </ComboBox>
                                                                </Grid>
                                                                <Grid Margin="0 5 0 0">
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="200" />
                                                                        <ColumnDefinition Width="100" />
                                                                    </Grid.ColumnDefinitions>
                                                                    <TextBlock Text="Shadow" TextWrapping="Wrap" Grid.Column="0" />
                                                                    <ComboBox IsEditable="True" x:Name="BossShadowResist" TextBoxBase.TextChanged="BossResist_TextChanged" PreviewTextInput="WholeNumberCheck" Grid.Column="1" SelectedIndex="1">
                                                                        <ComboBoxItem>0</ComboBoxItem>
                                                                        <ComboBoxItem>50</ComboBoxItem>
                                                                        <ComboBoxItem>100</ComboBoxItem>
                                                                        <ComboBoxItem>200</ComboBoxItem>
                                                                    </ComboBox>
                                                                </Grid>
                                                            </StackPanel>
                                                        </Border>
                                                    </Expander>
                                                </StackPanel>
                                            </Border>
                                        </Expander>
                                        <TextBlock Text="Player" Margin="0 5 0 0" />
                                        <Expander IsExpanded="True">
                                            <Border Margin="2" Padding="2" BorderBrush="#D6D6D6" BorderThickness="1" Background="#F2F2F2" CornerRadius="2">
                                                <StackPanel>
                                                    <Grid Margin="0 0 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Player Tanking" TextWrapping="Wrap" Grid.Column="0" />
                                                        <CheckBox x:Name="Tanking" Grid.Column="1" Checked="Tanking_Changed" Unchecked="Tanking_Changed"  />
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                            <ColumnDefinition Width="97*"/>
                                                            <ColumnDefinition Width="422*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Player Tanking Hit Frequency (s)" TextWrapping="Wrap" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,99,0" />
                                                        <ComboBox x:Name="TankingHitEvery" SelectedIndex="2" IsEditable="True" IsTextSearchEnabled="True" PreviewTextInput="NumberCheck" IsEnabled="False" Grid.Column="1" Grid.ColumnSpan="2" Margin="1,0,95,0">
                                                            <ComboBoxItem Content="1" />
                                                            <ComboBoxItem Content="2" />
                                                            <ComboBoxItem Content="3" />
                                                        </ComboBox>
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Player Tanking Hit Rage Gained" TextWrapping="Wrap" Grid.Column="0" />
                                                        <ComboBox x:Name="TankingRage" SelectedIndex="2" IsEditable="True" IsTextSearchEnabled="True" PreviewTextInput="NumberCheck" IsEnabled="False" Grid.Column="1">
                                                            <ComboBoxItem Content="10" />
                                                            <ComboBoxItem Content="20" />
                                                            <ComboBoxItem Content="30" />
                                                            <ComboBoxItem Content="40" />
                                                        </ComboBox>
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Player Unlimited Mana" TextWrapping="Wrap" Grid.Column="0" />
                                                        <CheckBox x:Name="UnlimitedMana" Grid.Column="1" />
                                                    </Grid>
                                                    <Grid Margin="0 10 0 0">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="200" />
                                                            <ColumnDefinition Width="100" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBlock Text="Player Unlimited Energy/Rage" TextWrapping="Wrap" Grid.Column="0" />
                                                        <CheckBox x:Name="UnlimitedResource" Grid.Column="1" />
                                                    </Grid>
                                                </StackPanel>
                                            </Border>
                                        </Expander>
                                    </StackPanel>
                                </TabItem>
                                <TabItem Header="Boss debuffs" IsEnabled="True">
                                    <TextBlock Margin="5 5 0 0" Text="/!\ UNDER DEVELOPMENT, edit the &quot;sim.json&quot; file manually then click the &quot;load&quot; button /!\" TextWrapping="Wrap" />
                                    <!--
